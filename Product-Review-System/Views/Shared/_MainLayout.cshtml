
<!DOCTYPE html>
<html>

<head>

    <meta name="viewport" content="width=device-width scale=1" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Style.css" rel="stylesheet" />



</head>
<body>
    <!-- Navbar-->
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class=" container">
            <div class="navbar-header">
                <button class="navbar-toggle collapsed" data-toggle="collapse"
                        data-target="#navbar">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="#" class="navbar-brand">
                    <img src="~/images/unnamed.png" class="logo-size" />
                </a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="#">
                            <i class="glyphicon glyphicon-home"></i>
                            Home
                        </a>
                    </li>

                    <li>
                        <a href="#">
                            <i class="glyphicon glyphicon-info-sign"></i>
                            About
                        </a>
                    </li>

                    <li>
                        <a href="#">
                            <i class="glyphicon glyphicon-earphone"></i>
                            Call Us
                        </a>
                    </li>
                </ul>

                <!--Partial Login-->
                @Html.Partial("_Login")

            </div>
        </div>
    </nav>

    <div class="container">
        <div class="raw">
            @RenderBody()
        </div>
    </div>


    <!--Footer-->
    <footer class="navbar navbar-fixed-bottom myfooter">
        <div class="container">
            <div class="raw">
                <div class="col-md-4">
                    <h3>
                        <i class=" glyphicon glyphicon-chevron-right "></i>
                        Product Review
                    </h3>
                    <p>

                        Hello, we are pleased for visiting you to our site.  " It's All about RATING "

                    </p>
                </div>

                <div class="col-md-4">
                    <h3>
                        <i class="glyphicon glyphicon-envelope"></i>
                        Contact Us
                    </h3>
                    <ul>
                        <li> <a href="#"> Facebook Page</a></li>
                        <li> <a href="#"> Twitter Page</a></li>
                        <li> <a href="#"> Text Us</a></li>
                    </ul>
                </div>

                <div class="col-md-4">
                    <h3>
                        <i class="  glyphicon glyphicon-globe "></i>
                        Links
                    </h3>
                    <ul>
                        <li> <a href="#"> Products </a></li>
                        <li> <a href="#"> Rates </a></li>
                        <li> <a href="#"> Feedbacks </a></li>
                    </ul>
                </div>
            </div>
            <!--  <div class="subfooter">
                    <p>
                        All rights reserved to the product Review site - 2020
                    </p>
                </div> -->

        </div>

    </footer>

    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>
@RenderSection("scripts", false)